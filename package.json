{
    "name": "kakeru_liff_member_demo",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "build": "rimraf server.js && rimraf ./backend && tsc",
        "dev": "nodemon -r dotenv/config ./backend-src/server.ts dotenv_config_path=./.env.ci",
        "start": "node server.js",
        "debug": "ts-node backend-src/server.ts",
        "test": "jest --coverage --runInBand",
        "test:watch": "jest --watch",
        "lint": "eslint --cache --fix \"backend-src/**/*.{ts,d.ts}\"",
        "deptestServer": "depcruise --exclude \"^node_modules\" --output-type dot ./backend-src/server.ts | dot -T svg -o dependency_graphs/dependencyGraph.svg",
        "db:init": "node ./backend/devOps/db/index.js --trace-warnings",
        "fs:init": "node ./backend/devOps/fs/index.js --trace-warnings",
        "clinic:doctor": "clinic doctor --on-port 'autocannon -c 10 -d 10 -m GET localhost:$PORT/api/m/categories' -- node server.js",
        "clinic:bubbleprof": "clinic bubbleprof --on-port 'autocannon -c 5 -a 500 -m GET localhost:$PORT/api/m/categories' -- node server.js",
        "pretty": "prettier --write ./backend-src",
        "prepare": "husky install"
    },
    "engines": {
        "node": ">=16.20.0"
    },
    "author": "Miraic",
    "license": "UNLICENSED",
    "private": "true",
