        "@line/bot-sdk": "^7.6.2",
        "ajv": "^8.11.0",
        "axios": "^1.4.0",
        "axios-retry": "^3.4.0",
        "bcryptjs": "^2.4.3",
        "connect-session-sequelize": "^7.1.5",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-session": "^1.17.2",
        "generate-password": "^1.7.0",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.0",
        "mime-types": "^2.1.35",
        "moment": "^2.29.4",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.11.0",
        "sequelize": "^6.37.3",
        "sharp": "^0.29.3",
        "socket.io": "^4.5.4",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.15",
        "@types/express-session": "^1.17.5",
